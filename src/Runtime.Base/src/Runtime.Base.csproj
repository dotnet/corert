    <Compile Condition="'$(Platform)'=='wasm'" Include="System\Runtime\StackFrameIterator.wasm.cs" />
  </ItemGroup>

  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), dir.targets))\dir.targets" />

  <PropertyGroup>
