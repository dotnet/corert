    <Compile Include="$(MetadataWriterPath)\MdBinaryWriterGen.cs" />
    <Compile Include="$(MetadataWriterPath)\NativeFormatWriterGen.cs" />
    <Compile Include="$(MetadataWriterPath)\NativeMetadataWriter.cs" />
  </ItemGroup>
</Project>
